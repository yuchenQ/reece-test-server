{
  "name": "reece-test-server",
  "version": "1.0.0",
  "description": "A Node.js server for Reece code challenge",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchenQ/reece-test-server.git"
  },
  "main": "src/index.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node src/index.js",
    "start:prod": "npm run build && babel-node build/index.js",
    "clean": "rm -rf build",
    "build": "npm run clean && babel src --out-dir build",
    "lint": "eslint './src/**/*.{jsx,js}' --quiet --fix"
  },
  "author": "Yuchen Qiao",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.8.4",
    "@koa/cors": "^3.0.0",
    "@koa/router": "^8.0.8",
    "dotenv": "^8.2.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-qs": "^2.0.0",
    "mongoose": "^5.9.3",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.3"
  }
}
